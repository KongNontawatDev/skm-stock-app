{
	"expo": {
		"name": "skm-stock-app",
		"slug": "skm-stock-app",
    "scheme": "skmstockapp",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"entryPoint": "./node_modules/expo-router/entry",
		"newArchEnabled": true,
		"plugins": [
			"expo-router"
		],
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"bundleIdentifier": "com.kongnontawat.skmstockapp",
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.kongnontawat.skmstockapp"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "ee985a79-476c-41d3-ad2f-a7beef4a1a0c"
			}
		},
		"owner": "kongnontawat"
	}
}
